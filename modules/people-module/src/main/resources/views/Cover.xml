<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	
	<grid model="com.axelor.people.db.Cover" title="Covers" name="cover_grid">
		<field name="name" />
		<field name="price"/>
		<field name="mobileSet" />	
	</grid>
	<form model="com.axelor.people.db.Cover" title="Cover" name="cover_form">
		<panel name="mainPanel" title="Main Panel">
			<field name="name"/>
			<field name="price"/>
			<field name="defaultValue" />
			<panel-related field="mobileSet" colSpan="25" hideIf="$popup()" />
		</panel>	
	</form>
		
</object-views>