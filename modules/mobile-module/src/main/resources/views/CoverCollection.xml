<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
				<grid model="com.axelor.mobile.db.CoverCollection" title="Cover List" name="covers_grid">
					<field name="name" />
					<field name="coverList" />
				</grid>
				<form model="com.axelor.mobile.db.CoverCollection" title="Cover Collection" name="covers_form">
					<panel>
						<field name="name" />
						<panel-related field="coverList" canSuggest="true"  />
					</panel>
				</form>
</object-views>
